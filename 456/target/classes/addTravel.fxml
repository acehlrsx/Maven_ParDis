<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox fx:id="travelPanelsContainer" alignment="CENTER_RIGHT" minHeight="92.0" onMouseClicked="#gotoShow" prefHeight="92.0" prefWidth="908.0" style="-fx-border-color: gray; -fx-border-radius: 30; -fx-background-color: white;" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="views.TravelController">
   <children>
      <Label fx:id="travelName" alignment="CENTER" contentDisplay="CENTER" prefHeight="26.0" prefWidth="719.0" text="Travel Name" textAlignment="CENTER">
         <font>
            <Font name="Segoe UI Bold" size="20.0" />
         </font>
      </Label>
      <Button fx:id="deleteItineraryBtn" mnemonicParsing="false" onAction="#deleteItinerary" prefHeight="140.0" prefWidth="140.0" style="-fx-background-color: #FCAB4E; -fx-background-radius: 30 0 0 30;" text="Delete" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font name="Segoe UI Bold" size="15.0" />
         </font>
      </Button>
      <Button fx:id="doneBtn" mnemonicParsing="false" onAction="#doneTravel" prefHeight="140.0" prefWidth="140.0" style="-fx-background-color: rgb( 224, 31, 147); -fx-background-radius: 0 30 30 0;" text="Done" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font name="Segoe UI Bold" size="15.0" />
         </font>
      </Button>
   </children>
</HBox>
