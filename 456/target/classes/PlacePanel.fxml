<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox prefWidth="948.0" spacing="10" style="-fx-border-color: gray; -fx-padding: 10;" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="views.addPlaceController">
    <TextField fx:id="placeTimeField" promptText="Enter Time" />
    <TextField fx:id="placeDestinationField" prefHeight="26.0" prefWidth="649.0" promptText="Enter your destination" />
    <Button fx:id="deletePlaceButton" onAction="#deletePlace" style="-fx-background-color: rgb( 224, 31, 147); -fx-background-radius: 50;" text="Delete" textFill="WHITE">
      <font>
         <Font name="Segoe UI Bold" size="12.0" />
      </font></Button>
</HBox>
